<div class="container">
    <div class="row">
        <div class="col s12 center-align">
            <h1>ALL SONGS</h1>
        </div>
    </div>
    <div class="row">
        <div class="col s2"></div>
        <div class="col s8">

            {{#each songs}}
            <h1>{{title}}</h1>
            <p>{{artist}}</p>
            <p>{{rating}}</p>
            <p>{{total_ratings}}</p>
            <p>{{url}}</p>
            <a class="waves-effect waves-yellow btn-large" href="/songs/{{id}}/edit">Edit Song</a>
            {{/each}}


        </div>
    </div>

</div>